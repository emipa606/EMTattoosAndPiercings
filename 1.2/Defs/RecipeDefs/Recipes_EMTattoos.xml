  <RecipeDef ParentName="EMTattoos">
    <defName>MakeEMSpeedSma</defName>
    <label>Design small cobra tattoo</label>
    <description>Prepare the pigments and materials ready for tattooing a speed enhancing design that is big enough to cover a hand or foot.</description>
    <jobString>Preparing tattoo.</jobString>
    <workAmount>1500</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>EMPigmentYellow</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <products>
      <I_EMSpeedSma>1</I_EMSpeedSma>
    </products>
    <researchPrerequisite>ResearchEMBasic</researchPrerequisite>
    <fixedIngredientFilter>
      <thingDefs>
        <li>EMPigmentYellow</li>
        <li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
  </RecipeDef>
